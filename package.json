{
    "name": "underscore.slice",
    "version": "1.2.0",
    "description": "Plugin for Underscore.js: slice array to deep-levels array",
    "license": "MIT",
    "author": {
        "name": "Piotr Kowalski",
        "email": "piecioshka@gmail.com",
        "url": "http://piecioshka.pl"
    },
    "scripts": {
        "clear": "rm -rf node_modules/",
        "build": "webpack --profile",
        "watch": "webpack -w",
        "test": "jasmine JASMINE_CONFIG_PATH=test/unit/jasmine.json",
        "coverage": "istanbul cover jasmine JASMINE_CONFIG_PATH=test/unit/jasmine.json",
        "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls -v"
    },
    "devDependencies": {
        "istanbul": "^0.4.5",
        "jasmine": "^2.5.2",
        "json-loader": "^0.5.4",
        "string-replace-loader": "^1.0.5",
        "underscore": "^1.8.3",
        "webpack": "^1.13.3"
    },
    "keywords": [
        "underscore",
        "slice",
        "array",
        "deep-level",
        "slice"
    ],
    "ignore": [
        "**/.*",
        "node_modules",
        "test"
    ],
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/piecioshka/underscore.slice.git"
    },
    "main": "dist/underscore.slice.js"
}
